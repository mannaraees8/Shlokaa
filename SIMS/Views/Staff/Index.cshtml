    <script src="~/Scripts/Chart/VehicleLog.js"></script>
    <script src="~/Scripts/Chart/SalesTrend.js"></script>
    <script src="~/Scripts/Chart/Sales.js"></script>
    <script src="~/Scripts/Chart/ProductCategory_Trend_Share.js"></script>
    <script src="~/Scripts/Chart/VisitLog_CollectionTrend_Collcetion.js"></script>
