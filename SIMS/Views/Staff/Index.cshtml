@section Scripts{
    <script>
        $(document).ready(function () {
            $.ajax({
                type: "GET",
                url: '/Chart/TallyPendingCount',
                success: function (data) {
                    if ($('#tallyEntyrPending'))
                        $('#tallyEntyrPending').html(data)
                }
            });
        })
    </script>
    <script src="~/Scripts/Chart/PaymentChequePending.js"></script>
    <script src="~/Scripts/Chart/SalesOrder.js"></script>
    <script src="~/Scripts/Chart/SpinningProduction.js"></script>
    <script src="~/Scripts/Chart/OrderStatus.js"></script>
